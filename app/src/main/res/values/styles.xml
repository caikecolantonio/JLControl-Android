<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- Base application theme. -->
<style name="TemaJlima" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Cor primária da ActionBar -->
    <item name="colorPrimary">@color/cinzolas</item>
    <!-- Cor primária escura da barra do sistema -->
    <item name="colorPrimaryDark">@color/purple_500</item>
    <!-- Cor de destaque -->
    <item name="colorAccent">@color/purple_700</item>

</style>
<style name="AppTheme.Toolbar" parent="TemaJlima">
    <item name="windowActionBar">false</item>
    <item name="windowNoTitle">true</item>
</style>
</resources>
